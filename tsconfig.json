{
	"compilerOptions": {
	  "outDir": "./dist",
	  "module": "CommonJS",
	  "target": "ES2020",
	  "sourceMap": true,
	  "esModuleInterop": true,
	  "moduleResolution": "node",
	  "resolveJsonModule": true,
	  "allowJs": true,
	  "strict": true,
	  "skipLibCheck": true
	},
	"include": ["src/**/*"]
  }
  